
@using WebApp_Prog3.Models.Clients;
@{
    var elemento = new TipoTrabajoClient();
    var elemento2 = elemento.GetAll();
        elemento2.OrderByDescending(x => x.Nombre);
    
    ViewBag.TipoTrabajo = elemento2;
}
<select id="Trabajo" onchange="TrabajoChange();" class="form-control">
    <option value="0" selected disabled>-- Tipo de libro --</option>
    
    
        @foreach (var m in ViewBag.TipoTrabajo)
        {
            <option value="@m.Id">@m.Nombre</option>
        }
    

</select>

