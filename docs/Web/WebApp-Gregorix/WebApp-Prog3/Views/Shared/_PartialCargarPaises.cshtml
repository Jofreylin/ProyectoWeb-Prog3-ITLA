
@using WebApp_Prog3.Models.Clients;
@{ 
    var elemento = new PaisClient();
    var elemento2 = elemento.GetAll();
    elemento2.OrderBy(x => x.Nombre);
    ViewBag.Pais = elemento2;
}
<select id="Pais" onchange="PaisChange();" class="form-control">
    <option value="0" selected disabled>-- Pais --</option>
    @foreach (var m in ViewBag.Pais)
    {
        <option value="@m.Id">@m.Nombre</option>
    }
</select>
